#include <stdio.h>
int main() {
 int a[10][10],i,j,row,col,sum=0;
 printf("enter row size");
 scanf("%d",&row);
 printf("enter column size");
 scanf("%d",&col);
 printf("enter elements");
 for(i=0;i<row;i++)
 {
     for(j=0;j<col;j++)
     {
         scanf("%d",&a[i][j]);
     }
 }
 for(i=0;i<row;i++)
 {
     for(j=0;j<col;j++)
     {
         sum=sum+a[j][i];
     }
     printf("%d col sum %d",i+1,sum);
     sum=0;
 }

    return 0;
}
